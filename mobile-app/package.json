{
  "name": "heirloom-mobile",
  "version": "1.0.0",
  "description": "Loominary Mobile App - Legacy Preservation for Future Generations",
  "main": "index.js",
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "test": "jest",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "build:android": "cd android && ./gradlew assembleRelease",
    "build:ios": "cd ios && xcodebuild -workspace LoominaryMobile.xcworkspace -scheme LoominaryMobile -configuration Release -destination generic/platform=iOS -archivePath LoominaryMobile.xcarchive archive"
  },
  "dependencies": {
    "react": "18.2.0",
    "react-native": "0.72.6",
    "react-native-vector-icons": "^10.0.0",
    "react-navigation": "^6.0.0",
    "@react-navigation/native": "^6.1.0",
    "@react-navigation/stack": "^6.3.0",
    "@react-navigation/bottom-tabs": "^6.5.0",
    "react-native-safe-area-context": "^4.7.0",
    "react-native-screens": "^3.25.0",
    "react-native-gesture-handler": "^2.13.0",
    "react-native-reanimated": "^3.5.0",
    "react-native-svg": "^13.14.0",
    "react-native-linear-gradient": "^2.8.0",
    "react-native-push-notification": "^8.1.0",
    "react-native-camera": "^4.2.0",
    "react-native-image-picker": "^7.0.0",
    "react-native-async-storage": "^1.19.0",
    "react-native-keychain": "^8.1.0",
    "react-native-biometrics": "^3.0.0",
    "react-native-share": "^9.4.0",
    "react-native-webview": "^13.6.0",
    "axios": "^1.5.0",
    "react-query": "^3.39.0",
    "zustand": "^4.4.0"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/preset-env": "^7.20.0",
    "@babel/runtime": "^7.20.0",
    "@react-native/eslint-config": "^0.72.0",
    "@react-native/metro-config": "^0.72.0",
    "@tsconfig/react-native": "^3.0.0",
    "@types/react": "^18.0.24",
    "@types/react-test-renderer": "^18.0.0",
    "babel-jest": "^29.2.1",
    "eslint": "^8.19.0",
    "jest": "^29.2.1",
    "metro-react-native-babel-preset": "0.76.8",
    "prettier": "^2.4.1",
    "react-test-renderer": "18.2.0",
    "typescript": "4.8.4"
  },
  "engines": {
    "node": ">=16"
  }
}